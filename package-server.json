{
  "name": "amazon-dhc-onboarding-server",
  "version": "1.0.0",
  "description": "Amazon DHC Onboarding Tool - Web Server Version",
  "main": "server.js",
  "scripts": {
    "start": "npm run build-css && node server.js",
    "start-fast": "node server.js",
    "build-css": "npx tailwindcss -i ./styles-tailwind.css -o ./styles-compiled.css",
    "build-css-watch": "npx tailwindcss -i ./styles-tailwind.css -o ./styles-compiled.css --watch",
    "install-deps": "npm install express multer"
  },
  "keywords": [
    "amazon",
    "onboarding",
    "web-server",
    "health"
  ],
  "author": "Amazon DHC Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}





